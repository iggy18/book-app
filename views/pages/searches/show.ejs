<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% arrayOfBooks.forEach(book => { %>
        <div>
            <img src="<%= book.cover %>">
            <h2><%= book.title %></h2>
            <% book.authors.forEach(author => { %>
                <h3><%= author %></h3>
            <% }); %>
            <p><%= book.description %></p> 
        </div>
    <% }); %>
</body>
</html>